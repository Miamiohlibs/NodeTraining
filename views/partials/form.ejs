<form action="/output" method="POST">
    <div class="form-group">
        <label for="en">Location</label>
        <!-- to allow user to select location, simply switch form-select to input text -->
        <!-- <input type="text" class="form-control" name="en" id="en" placeholder="search"> -->
        <!-- if value == station.name add selected tag -->
        <select class="form-control" name="en" id="en">
            <% options.forEach( entry => { %>
                <option value="<%- entry.value %>">
                <%- entry.option %>
                </option>
            <% }); %>
        </select>
    </div>
    <div class="form-group">
        <label for="start">Start Date</label><br>
        <input type="date" class="form-control" name="start" id="start">
    </div>
    <div class="form-group">
        <label for="end">End Date</label><br>
        <input type="date" class="form-control" name="end" id="end">
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-dark">Submit</button>
    </div>
</form>